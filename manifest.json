{
  "name": "History Tracker",
  "version": "0.0.1",
  "description": "A smart web history",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["history", "storage", "bookmarks"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon_48dp.png"
  },
  "icons": {
    "48": "images/icon_48dp.png"
  }
}
